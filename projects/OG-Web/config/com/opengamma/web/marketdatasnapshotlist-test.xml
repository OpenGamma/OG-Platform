  <bean id="snapshotMaster" class="com.opengamma.master.marketdatasnapshot.impl.InMemorySnapshotMaster"/>

  <bean id="updateManager" class="com.opengamma.web.server.push.subscription.TestRestUpdateManager"/>

</beans>