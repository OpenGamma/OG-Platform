<?xml version="1.0" encoding="UTF-8"?>
<bean type="com.opengamma.financial.analytics.curve.ISDAYieldCurveDefinition">
 <uniqueId>DbCfg~2261220~543</uniqueId>
 <name>ISDA YC EUR</name>
 <!-- 
  curve-level convention includes:
    - ccy
    - business day convention ('bad day convention')
    - holiday calendar (w/e only for most calendars)
    - spot date
  -->
 <conventionLink type="com.opengamma.core.link.ResolvableConventionLink">
  <identifier>
   <identifier>CONVENTION~ISDA YC EUR</identifier>
   <type>com.opengamma.financial.convention.ISDAYieldCurveConvention</type>
  </identifier>
 </conventionLink>
 <nodes>
  <item type="com.opengamma.financial.analytics.curve.credit.node.ISDACashNode">
   <tenor>P1D</tenor>
   <!-- 
    cash convention includes:
     - ccy
     - day count
   -->
	 <conventionLink type="com.opengamma.core.link.ResolvableConventionLink">
	  <identifier>
	   <identifier>CONVENTION~ISDA YC EUR CASH</identifier>
	   <type>com.opengamma.financial.convention.ISDACashNodeConvention</type>
	  </identifier>
	 </conventionLink>
   <ticker>BLOOMBERG_TICKER~EUDR1T Curncy</ticker>
   <dataField></dataField>
   <dataFieldType></dataFieldType>
  </item>
  <item type="com.opengamma.financial.analytics.curve.credit.node.ISDASwapNode">
   <tenor>P3M</tenor>
   <!-- 
    all convention fields apply to fixed leg since float leg conventions
    not used in curve bootstrapping. fields include:
      - ccy
      - payment frequency
      - day count
   -->
   <conventionLink type="com.opengamma.core.link.ResolvableConventionLink">
    <identifier>
     <identifier>CONVENTION~ISDA YC EUR SWAP FIX</identifier>
     <type>com.opengamma.financial.convention.ISDACashNodeConvention</type>
    </identifier>
   </conventionLink>
   <ticker>BLOOMBERG_TICKER~EUDRC Curncy</ticker>
   <dataField></dataField>
   <dataFieldType></dataFieldType>
  </item>
 </nodes>
</bean>
